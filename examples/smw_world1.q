      <q:set name="score" value="{0}" />
      <q:set name="coins" value="{0}" />
      <q:set name="lives" value="{5}" />
      <q:set name="timer" value="{300}" />
      <q:set name="dragonCoins" value="{0}" />
      <q:set name="gameOver" value="{false}" />
      <q:set name="won" value="{false}" />
      <q:set name="marioState" value="small" />
      <q:set name="spinJumping" value="{false}" />
      <q:set name="checkpointX" value="{40}" />
      <q:set name="checkpointY" value="{384}" />
      <q:set name="timerCounter" value="{0}" />
      _updateHUD();
    </q:function>

    <!-- === SPIN JUMP === -->
    <q:function name="spinJumpStart">
      spinJumping = true;
      var mb = _sprites["mario"].body;
      if (mb) {
        Matter.Body.setVelocity(mb, {x: mb.velocity.x, y: -20});
      }
      game.play('smw_spin_sfx');
    </q:function>

    <!-- === INPUT === -->
    <qg:input key="r" action="restart" type="press" />
    <qg:input key="x" action="spinJumpStart" type="press" />

  </qg:scene>
</q:application>
