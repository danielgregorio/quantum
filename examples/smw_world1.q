    <!-- === TIMER LOGIC === -->
    <q:function name="_timerTick">
      if (gameOver || won) return;
      timerCounter++;
      if (timerCounter &gt;= 60) {
        timerCounter = 0;
        timer--;
        if (timer &lt;= 0) {
          timer = 0;
          lives--;
          if (lives &lt;= 0) {
            gameOver = true;
          } else {
            game.respawn("mario", checkpointX, checkpointY);
            timer = 300;
          }
        }
        _updateHUD();
      }
    </q:function>

