<q:application id="smw_world1" type="game" engine="2d">

  <!-- === BEHAVIORS === -->

  <qg:behavior name="MarioBehavior">
    <q:set name="marioState" value="small" type="string" />
    <q:set name="spinJumping" value="false" type="boolean" />

    <q:function name="onEnemyHit">
      var other = this._other;
      var myBody = this.owner.body;
      var enemyBody = other ? other.body : null;
      var stomped = false;
      if (myBody &amp;&amp; enemyBody) {
        var marioBottom = myBody.position.y + 16;
        var enemyCenter = enemyBody.position.y;
        stomped = (marioBottom &lt; enemyCenter + 4) &amp;&amp; (myBody.velocity.y &gt; 0);
      }
      if (stomped || spinJumping) {
        Matter.Body.setVelocity(myBody, {x: myBody.velocity.x, y: -10});
        /* Check if enemy is a Rex (has onStomp handler) */
        var enemySprite = other;
        if (enemySprite &amp;&amp; enemySprite._behaviors) {
          var hasStomp = false;
          for (var i = 0; i &lt; enemySprite._behaviors.length; i++) {
            if (enemySprite._behaviors[i].onStomp) {
              enemySprite._behaviors[i].onStomp();
              hasStomp = true;
              break;
            }
          }
          if (!hasStomp) {
            game.destroy(other);
            score += 200;
            game.play('smw_stomp_sfx');
          }
        } else {
          game.destroy(other);
          score += 200;
          game.play('smw_stomp_sfx');
        }
        _updateHUD();
      } else {
        /* Take damage */
        if (marioState === "big" || marioState === "cape") {
          marioState = "small";
          game.play('smw_powerup_sfx');
          _updateHUD();
        } else {
          lives--;
          if (lives &lt;= 0) {
            gameOver = true;
            _updateHUD();
          } else {
            game.respawn("mario", checkpointX, checkpointY);
            marioState = "small";
            _updateHUD();
          }
        }
      }
    </q:function>

    <q:function name="onCoinGet">
      var other = this._other;
      coins++;
      score += 200;
      game.destroy(other);
      game.play('smw_coin_sfx');
      if (coins &gt;= 100) {
        coins -= 100;
        lives++;
      }
      _updateHUD();
    </q:function>

    <q:function name="onDragonCoinGet">
      var other = this._other;
      dragonCoins++;
      score += 2000;
      game.destroy(other);
      game.play('smw_coin_sfx');
      if (dragonCoins &gt;= 5) {
        lives++;
      }
      _updateHUD();
    </q:function>

    <q:function name="onBlockHit">
      var other = this._other;
      var myBody = this.owner.body;
      if (myBody &amp;&amp; other &amp;&amp; other.body) {
        var myY = myBody.position.y;
        var blockY = other.body.position.y;
        if (myY &gt; blockY + 4) {
          Matter.Body.setVelocity(myBody, {x: myBody.velocity.x, y: 2});
        }
      }
    </q:function>

    <q:function name="onDeath">
      lives--;
      if (lives &lt;= 0) {
        gameOver = true;
      } else {
        game.respawn("mario", checkpointX, checkpointY);
        marioState = "small";
      }
      _updateHUD();
    </q:function>

    <q:function name="onGoalTape">
      if (!won) {
        won = true;
        game.stop('smw_overworld_bgm');
        game.play('smw_goal_sfx');
        /* Score based on tape position */
        var marioBody = _sprites["mario"].body;
        if (marioBody) {
          var hitY = marioBody.position.y;
          var bonus = Math.max(0, Math.floor((400 - hitY) * 10));
          score += bonus;
        }
        _updateHUD();
        /* Disable controls and walk right */
        var mb = _sprites["mario"].body;
        if (mb) {
          var walkInterval = setInterval(function() {
            Matter.Body.setVelocity(mb, {x: 3, y: mb.velocity.y});
          }, 16);
          setTimeout(function() { clearInterval(walkInterval); }, 3000);
        }
      }
    </q:function>

    <q:function name="onMidwayGate">
      checkpointX = 1760;
      checkpointY = 384;
      var other = this._other;
      if (other) game.destroy(other);
    </q:function>

    <q:function name="onMushroomGet">
      var other = this._other;
      if (marioState === "small") {
        marioState = "big";
      }
      score += 1000;
      game.destroy(other);
      game.play('smw_powerup_sfx');
      _updateHUD();
    </q:function>

    <q:function name="onCapeFeatherGet">
      var other = this._other;
      marioState = "cape";
      score += 1000;
      game.destroy(other);
      game.play('smw_powerup_sfx');
      _updateHUD();
    </q:function>
  </qg:behavior>

  <qg:behavior name="KoopaAI">
    <q:set name="dir" value="-1" type="integer" />
    <q:set name="spd" value="0.8" type="float" />

    <q:function name="update">
      var b = this.owner.body;
      if (b) Matter.Body.setVelocity(b, {x: this.dir * this.spd, y: b.velocity.y});
    </q:function>

    <q:function name="onWall">
      this.dir *= -1;
    </q:function>
  </qg:behavior>

  <qg:behavior name="KoopaRedAI">
    <q:set name="dir" value="-1" type="integer" />
    <q:set name="spd" value="0.8" type="float" />

    <q:function name="update">
      var b = this.owner.body;
      if (b) Matter.Body.setVelocity(b, {x: this.dir * this.spd, y: b.velocity.y});
    </q:function>

    <q:function name="onWall">
      this.dir *= -1;
    </q:function>

    <q:function name="onEdge">
      this.dir *= -1;
    </q:function>
  </qg:behavior>

  <qg:behavior name="RexAI">
    <q:set name="dir" value="-1" type="integer" />
    <q:set name="spd" value="0.6" type="float" />
    <q:set name="hits" value="0" type="integer" />

    <q:function name="update">
      var b = this.owner.body;
      if (b) Matter.Body.setVelocity(b, {x: this.dir * this.spd, y: b.velocity.y});
    </q:function>

    <q:function name="onWall">
      this.dir *= -1;
    </q:function>

    <q:function name="onStomp">
      this.hits++;
      if (this.hits &gt;= 2) {
        game.destroy(this.owner);
        score += 400;
        game.play('smw_stomp_sfx');
      } else {
        /* Squish: reduce height visually */
        var spr = this.owner.sprite;
        if (spr) {
          spr.height = 16;
          spr.y += 16;
        }
        score += 100;
        game.play('smw_stomp_sfx');
      }
      _updateHUD();
    </q:function>
  </qg:behavior>

  <qg:behavior name="PiranhaAI">
    <q:set name="baseY" value="0" type="float" />
    <q:set name="amplitude" value="48" type="float" />
    <q:set name="timer" value="0" type="float" />
    <q:set name="initialized" value="false" type="boolean" />

    <q:function name="update">
      if (!this.initialized) {
        this.baseY = this.owner.sprite ? this.owner.sprite.y : 0;
        this.initialized = true;
      }
      /* Pause if Mario is close horizontally */
      var mario = _sprites["mario"];
      if (mario &amp;&amp; mario.body) {
        var dx = Math.abs(mario.body.position.x - (this.owner.body ? this.owner.body.position.x : this.owner.sprite.x));
        if (dx &lt; 32) return;
      }
      this.timer += 0.03;
      var newY = this.baseY + Math.sin(this.timer) * this.amplitude;
      var spr = this.owner.sprite;
      if (spr) spr.y = newY;
      if (this.owner.body) {
        Matter.Body.setPosition(this.owner.body, {x: this.owner.body.position.x, y: newY});
      }
    </q:function>
  </qg:behavior>

  <qg:behavior name="QBlock">
    <q:set name="active" value="true" type="boolean" />

    <q:function name="onHit">
      if (!this.active) return;
      var other = this._other;
      if (!other || !other.body) return;
      var playerY = other.body.position.y;
      var blockY = this.owner.body ? this.owner.body.position.y : this.owner.sprite.y;
      if (playerY &gt; blockY + 4) {
        this.active = false;
        coins++;
        score += 200;
        game.play('smw_coin_sfx');
        if (coins &gt;= 100) { coins -= 100; lives++; }
        _updateHUD();
        var spr = this.owner.sprite;
        if (spr) {
          spr.texture = PIXI.Texture.from('used_tile.png');
          var origY = spr.y;
          spr.y -= 8;
          setTimeout(function() { spr.y = origY; }, 150);
        }
      }
    </q:function>
  </qg:behavior>

  <qg:behavior name="QBlockMushroom">
    <q:set name="active" value="true" type="boolean" />

    <q:function name="onHit">
      if (!this.active) return;
      var other = this._other;
      if (!other || !other.body) return;
      var playerY = other.body.position.y;
      var blockY = this.owner.body ? this.owner.body.position.y : this.owner.sprite.y;
      if (playerY &gt; blockY + 4) {
        this.active = false;
        /* Give powerup directly */
        if (marioState === "small") {
          marioState = "big";
        }
        score += 1000;
        game.play('smw_powerup_sfx');
        _updateHUD();
        var spr = this.owner.sprite;
        if (spr) {
          spr.texture = PIXI.Texture.from('used_tile.png');
          var origY = spr.y;
          spr.y -= 8;
          setTimeout(function() { spr.y = origY; }, 150);
        }
      }
    </q:function>
  </qg:behavior>

  <qg:behavior name="QBlockCapeFeather">
    <q:set name="active" value="true" type="boolean" />

    <q:function name="onHit">
      if (!this.active) return;
      var other = this._other;
      if (!other || !other.body) return;
      var playerY = other.body.position.y;
      var blockY = this.owner.body ? this.owner.body.position.y : this.owner.sprite.y;
      if (playerY &gt; blockY + 4) {
        this.active = false;
        marioState = "cape";
        score += 1000;
        game.play('smw_powerup_sfx');
        _updateHUD();
        var spr = this.owner.sprite;
        if (spr) {
          spr.texture = PIXI.Texture.from('used_tile.png');
          var origY = spr.y;
          spr.y -= 8;
          setTimeout(function() { spr.y = origY; }, 150);
        }
      }
    </q:function>
  </qg:behavior>

  <qg:behavior name="GoalTapeAI">
    <q:set name="baseY" value="0" type="float" />
    <q:set name="dir" value="1" type="integer" />
    <q:set name="spd" value="1.5" type="float" />
    <q:set name="minY" value="272" type="float" />
    <q:set name="maxY" value="384" type="float" />
    <q:set name="initialized" value="false" type="boolean" />

    <q:function name="update">
      if (!this.initialized) {
        this.baseY = this.owner.sprite ? this.owner.sprite.y : 272;
        this.initialized = true;
      }
      var spr = this.owner.sprite;
      if (!spr) return;
      spr.y += this.dir * this.spd;
      if (spr.y &lt;= this.minY) { spr.y = this.minY; this.dir = 1; }
      if (spr.y &gt;= this.maxY) { spr.y = this.maxY; this.dir = -1; }
      if (this.owner.body) {
        Matter.Body.setPosition(this.owner.body, {x: this.owner.body.position.x, y: spr.y});
      }
    </q:function>
  </qg:behavior>

  <!-- === SCENE === -->

  <qg:scene name="yoshis_island_1" width="3200" height="448" background="#5CA0FF">

    <qg:physics gravity-y="10" bounds="none" />

    <qg:camera follow="mario" lerp="0.08" bounds="scene" />

    <!-- State variables -->
    <q:set name="score" value="0" type="integer" />
    <q:set name="coins" value="0" type="integer" />
    <q:set name="lives" value="5" type="integer" />
    <q:set name="timer" value="300" type="integer" />
    <q:set name="dragonCoins" value="0" type="integer" />
    <q:set name="gameOver" value="false" type="boolean" />
    <q:set name="won" value="false" type="boolean" />
    <q:set name="marioState" value="small" type="string" />
    <q:set name="spinJumping" value="false" type="boolean" />
    <q:set name="checkpointX" value="40" type="integer" />
    <q:set name="checkpointY" value="384" type="integer" />
    <q:set name="timerCounter" value="0" type="integer" />

