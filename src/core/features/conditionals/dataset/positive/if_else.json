{
  "dataset_type": "positive",
  "feature": "conditionals",
  "category": "if_else",
  "description": "Valid examples of if-else statements (binary choices)",
  "examples": [
    {
      "id": "if-else-001",
      "intent": "Temperature check with two outcomes",
      "input": {
        "natural_language": "Show 'warm' if temperature > 20, otherwise show 'cold'",
        "context": {
          "available_vars": ["temperature"],
          "scope": "component"
        }
      },
      "expected_output": {
        "quantum_code": "<q:if condition=\"{temperature} > 20\">\n  It's warm.\n<q:else/>\n  It's cold.\n</q:if>",
        "ast": {
          "type": "IfNode",
          "attributes": {
            "condition": "{temperature} > 20",
            "has_elseif": false,
            "has_else": true
          }
        },
        "runtime_behavior": "Shows one of two messages based on temperature"
      },
      "tags": ["if_else", "binary_choice", "numeric"]
    },
    {
