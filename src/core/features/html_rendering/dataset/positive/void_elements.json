    {
      "id": "void-004",
      "intent": "Render horizontal rule",
      "input": {
        "natural_language": "Add hr separator",
        "context": {
          "element": "hr"
        }
      },
      "expected_output": {
        "quantum_code": "<q:component name=\"Section\">\n  <p>Section 1</p>\n  <hr />\n  <p>Section 2</p>\n</q:component>",
        "ast": {
          "type": "ComponentNode",
          "statements": [
            {
              "type": "HTMLNode",
              "tag": "p"
            },
            {
              "type": "HTMLNode",
              "tag": "hr",
              "self_closing": true
            },
            {
              "type": "HTMLNode",
              "tag": "p"
            }
          ]
        },
        "rendered_html": "<p>Section 1</p>\n<hr />\n<p>Section 2</p>",
